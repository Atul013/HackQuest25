# Environment variables for Live Audio Transcription
# Copy this file to .env and fill in your actual Supabase credentials

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Audio Configuration (Optional - defaults are set in the code)
AUDIO_CHUNK_SIZE=1024
AUDIO_SAMPLE_RATE=16000
AUDIO_RECORD_SECONDS=5
AUDIO_CHANNELS=1

# Cleanup Configuration
CLEANUP_INTERVAL_MINUTES=10
CLEANUP_CHECK_INTERVAL_SECONDS=60

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=audio_transcription.log

# Whisper Model Configuration
WHISPER_MODEL=base  # Options: tiny, base, small, medium, large